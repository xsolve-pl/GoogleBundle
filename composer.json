{
    "name": "xsolve-pl/google-bundle",
    "description": "Fork of antimattr/google-bundle and drymek/google-bundle with Javascript Maps added.",
    "keywords": ["google", "analytics", "adwords", "maps"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name":     "xsolve-pl",
            "homepage": "https://github.com/xsolve.pl"
        }
    ],
    "require": {
        "php":             ">=5.3.0",
        "symfony/symfony": ">=2.0"
    },
    "replace" : {
        "antimattr\\GoogleBundle": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "AntiMattr\\GoogleBundle": ""
        }
    },
    "target-dir" : "AntiMattr/GoogleBundle"
}
